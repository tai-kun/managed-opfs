{
  "excludes": [
    "**/.Trash/",
    "**/.history/",
    "**/node_modules/",
    "**/*-lock.json"
  ],
  "lineWidth": 100,
  "typescript": {
    "trailingCommas": "onlyMultiLine",
    "singleBodyPosition": "nextLine",
    "arrowFunction.useParentheses": "preferNone"
  },
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.95.8.wasm",
    "https://plugins.dprint.dev/json-0.20.0.wasm"
  ]
}
